<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Firmstep Test Suite" verbose="1" parallel="tests"
       thread-count="3" >
    <test name="WIN10TEST" annotations="JDK"
          preserve-order="true">
        <parameter name="browser" value="Internet Explorer"/>
        <parameter name="version" value="11.0"/>
        <parameter name="platform" value="WIN10"/>
        <classes>
            <class name="com.SELF.Runner.TestRunner" />
        </classes>
    </test> <!-- Test -->
    <test name="WIN8TEST" annotations="JDK"
          preserve-order="true">
        <parameter name="browser" value="chrome"/>
        <parameter name="version" value="73.0"/>
        <parameter name="platform" value="WIN10"/>
        <classes>
            <class name="com.SELF.Runner.TestRunner" />
        </classes>
    </test> <!-- Test -->
    <test name="MACTEST" annotations="JDK"
          preserve-order="true">
        <parameter name="browser" value="safari"/>
        <parameter name="version" value="12.0"/>
        <parameter name="platform" value="macos 10.14"/>
        <classes>
            <class name="com.SELF.Runner.TestRunner" />
        </classes>
    </test> <!-- Test -->

</suite>